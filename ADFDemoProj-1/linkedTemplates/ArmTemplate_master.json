{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFDemoProj-1"
		},
		"SqlDBSanProj_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlDBSanProj_LS'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=testserversan.database.windows.net;Initial Catalog=SqlDBProjSan;User ID=sandhyaadmin"
		},
		"storageaccountprojsan_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'storageaccountprojsan_LS'"
		},
		"DemoProjectAzureKV_LS_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://DemoProjKeyVault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFDemoProj-1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"storageaccountprojsan_LS_connectionString": {
						"value": "[parameters('storageaccountprojsan_LS_connectionString')]"
					},
					"SqlDBSanProj_LS_connectionString": {
						"value": "[parameters('SqlDBSanProj_LS_connectionString')]"
					},
					"DemoProjectAzureKV_LS_properties_typeProperties_baseUrl": {
						"value": "[parameters('DemoProjectAzureKV_LS_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}